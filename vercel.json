{
    "builds": [{
        "src": "Django_Final/wsgi.py",
        "use": "@vercel/python",
        "config": {"runtime": "python3.10" }
    },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles_build" }
    
    }],
    "routes": [
        {   
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Django_Final/wsgi.py"
        }
    ]
}